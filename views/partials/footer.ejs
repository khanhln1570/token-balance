<div class="modal fade" id="modal-select-image" tabindex="-1" role="dialog" aria-labelledby="modal-select-imageTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Import NFT</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" style="text-align: center;">
        <img id="image-detail" width="250" height="250">
        <h5 id="image-title"></h5>
        <p id="image-tokenId"></p>
        <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
        <label class="form-check-label" for="flexCheckDefault">
          I'm printing NFTs that I own for my personal use, only, and not for resale.
        </label>
      </div>
      <div class="modal-footer">
        <button id="callZekeke" type="button" class="btn btn-primary" disabled><i class="fa-solid fa-file-import"></i>
          Import NFT</button>
        <form method="post" action="/qr/generate">
          <button id="generateQr" type="submit" class="btn btn-primary" disabled><i class="fa-solid fa-qrcode"></i>
            Generate QR code</button>
          <input id="qr-url" type="text" name="url" hidden>
        </form>
        <% if(global.version==='v2' ) {%>
          <button id="downloadNft" type="button" class="btn btn-primary" disabled><i
              class="fa-solid fa-file-download"></i> Download</button>
          <%}%>

      </div>
    </div>
  </div>
</div>
<% if(global.version==='v1' ) {%>
  <p class="text-center text-muted">&copy; Kustomly 2023</p>
  <%} else {%>
    <div class="container">
      <h1>TOP SEARCH</h1>
      <div class="wrap">
        <% global.defaultCollections.forEach(function(collection) { %>
          <div class="card nft"
            data-link="https://svc.blockdaemon.com/nft/v1/ethereum/mainnet/media/<%=collection.logo%>?apiKey=rBWqZx0eHdgw8DTBLKL0gDBVQz6FCm4C969AsgV15MvFB3Z4"
            data-title="<%=collection.name%>" data-tokenid="<%=collection.token_id%> ">
            <img src='https://i.pinimg.com/originals/3f/2c/97/3f2c979b214d06e9caab8ba8326864f3.gif'
              data-src="https://svc.blockdaemon.com/nft/v1/ethereum/mainnet/media/<%=collection.logo%>?apiKey=rBWqZx0eHdgw8DTBLKL0gDBVQz6FCm4C969AsgV15MvFB3Z4"
              class="card-img-top lazy-image" alt="...">
            <div class="card-body">
              <h5 class="card-title">
                <%=collection.name%>
              </h5>
            </div>
          </div>
          <% }); %>
      </div>
    </div>

    <%}%>
      <script src="/index.js"></script>